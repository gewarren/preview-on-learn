{
  "manifest_version": 3,
  "name": "Preview on Learn",
  "version": "1.0.0",
  "description": "Add 'Preview on Learn' option to GitHub pull request file kebab menus",
  "icons": {
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://github.com/*/*/pull/*/files"
      ]
    }
  ]
}
